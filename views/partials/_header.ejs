<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-success">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup"
      aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
      </div>
    </div>
    <!--If user has been set-->
    <% if (user) { %>
    <!--prints the user (which has been set to a cookie and carried as an object with the value user-->
    <div style="padding-right:  15px; padding-top: 7px;">
      <h5 style="color: aliceblue"> <%= user.email %> </h5>
    </div>
    <!--creates the logout button-->
    <form method="POST" action="/logout">
      <button type="submit">Logout</button>
    </form>
    <% } else { %>
    <!-- below is my signup button with post method that should redirect to register -->
    <form method="GET" action="/register">
      <button type="submit">Sign Up</button>
    </form>
    <!-- login button -->
    <form method="GET" action="/urls_login">
      <button type="submit">Login</button>
    </form>
    <% } %>
  </nav>
</header>